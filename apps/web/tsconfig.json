{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": ["vite/client"],
    "rootDirs": ["."],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@cricket247/types": ["../../packages/types/src/index.ts"],
      "@cricket247/types/*": ["../../packages/types/src/*"]
    },
    "strictNullChecks": true
  },
  "references": [
    {
      "path": "../server"
    },
    {
      "path": "../../packages/types"
    }
  ]
}
